            to_send.xu_chau_luot = $("form textarea#xu-chau-luot").val()
            to_send.luu_y = $("form textarea#luu-y").val()
            to_send.under_title = $("form textarea#under-title").val()

            console.log(to_send)
            $.get("/update-ngay-lich",to_send, function(data, status){
