            if(confirm("Có chắc chắn muốn xóa "+ $("input#title").val())){
                $.get("/delete-ngay-lich",{_id: $("input#_id").val()}, function(data, status){
                    if(status == "success"){
                        alert("Xóa thành công!")
                        location.reload()
                    }else{
                        alert("Xóa thất bại!")
                    }
                })
            }else{
                alert("Bạn đã hủy bỏ việc xóa "+  $("input#title").val())
            }
        })
