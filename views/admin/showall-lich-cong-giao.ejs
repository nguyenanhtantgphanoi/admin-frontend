            let bd_2 = $("#bd-2").val()
            let tin_mung = $("#tin-mung").val()

            let loi_chua = "<p>"+bd_1+"; "+bd_2+"; "+tin_mung+"</p>"
            let cau_loi_chua = "<p>"+$("#cau-loi-chua")+"</p>"

            let htmlStr = title + bac_le + mau_ao_le + loi_chua +cau_loi_chua
            $("#view-in-lich").append(htmlStr)
        }
